{
  "name": "ringcentral-engage-client",
  "version": "0.4.1",
  "description": "Simple JavaScript wrapper for RingCentral Engage API.",
  "author": "Drake Zhao <drake.zhao@ringcentral.com>",
  "license": "MIT",
  "main": "dist/ringcentral-engage-digital",
  "module": "src/ringcentral-engage-digital.ts",
  "types": "dist/ringcentral-engage-digital",
  "scripts": {
    "test": "standard && jest --coverage",
    "fix": "standard --fix",
    "release": "webpack --progress --colors",
    "prepublishOnly": "npm run release",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.9.0",
    "@tsconfig/recommended": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "concat-stream": "^2.0.0",
    "coveralls": "^3.1.0",
    "dotenv-override-true": "^6.2.0",
    "form-data": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "ramda": "^0.26.1",
    "standard": "^14.3.4",
    "timeout-as-promise": "^1.0.0",
    "ts-loader": "^8.0.11",
    "ts-standard": "^9.0.0",
    "typescript": "^4.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "./test"
    ],
    "setupFiles": [
      "dotenv-override-true/config"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*-compiled.js",
      "/dist/"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ringcentral/engage-digital-js.git"
  },
  "keywords": [
    "ringcentral",
    "engage",
    "digital",
    "js",
    "sdk"
  ],
  "bugs": {
    "url": "https://github.com/ringcentral/engage-digital-js/issues"
  },
  "homepage": "https://github.com/ringcentral/engage-digital-js#readme"
}
